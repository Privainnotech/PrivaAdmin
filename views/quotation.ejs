    <div class="main">
      <%- include('./partials/navbar'); %>

      <main class="content">
        <div class="container-fluid p-0">
          <div class="row">
            <div class="col-12">
              <div class="card flex-fill">
                <div class="card-header title">
                  <h1>Quotation List</h1>
                  <button type="button" class="btn btn-success" id="addProject">New Project</button>
                </div>
                <table id="tableQuo" class="table table-hover my-0 ">
                  <thead>
                    <tr>
                      <th>Index</th>
                      <th>Quotation NO.</th>
                      <th>Revised</th>
                      <th>Project</th>
                      <th>Customer</th>
                      <th>Quotation Date</th>
                      <th>Update Date</th>
                      <th>Status</th>
                      <th>Editor</th>
                      <th>Action</th>
                      <th style="display: none;"></th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td colspan="9">Loading...</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-5 col-sm-12 col-md-12 col-lg-5 col-xxl-5">
              <div class="card flex-fill w-100">
                <div class="card-header title v-2">
                  <h2 id="ProNo">Project No.</h2>
                  <div class="btn-group right" role="group" aria-label="Basic example">
                    <button type="button" class="btn btn-success invisible" id="modalEditProject">
                      <h3 id="btn-text">Edit</h3>
                    </button>
                    <button type="button" class="btn btn-dark btn-rad" id="btnPrint">Print</button>
                    <button type="button" class="btn btn-dark btn-rad" id="btnRevised">Revised</button>
                  </div>
                </div>
                <div class="card-body w-100">
                  <div class="align-self-center chart chart-lg">
                    <!-- set -->
                    <div class="input-group">
                      <span class="input-group-text group-title mg-0 f-9">Project Name:</span>
                      <input type="text" class="form-control group-title edit f-9" id="PJ_Name" disabled>
                    </div>

                    <div class="input-group">
                      <span class="input-group-text mg-0">Customer:</span>
                      <input id="CusName" class="no-bor form-control" disabled>
                    </div>

                    <div class="input-group">
                      <span class="input-group-text mg-0">Date:</span>
                      <input id="QDate" class="no-bor form-control disabled" disabled>
                    </div>

                    <div class="input-group ">
                      <span class="input-group-text mg-0">Email: </span>
                      <input id="CusEmail" class="no-bor form-control " disabled>
                    </div>

                    <div class="input-group ">
                      <span class="input-group-text mg-0">Company:</span>
                      <input id="ComName" class="no-bor form-control " disabled>
                    </div>

                    <div class="input-group ">
                      <textarea style="width: 94px;" class="input-group-text mg-0" rows="1" cols="1"
                        disabled>Address:</textarea>
                      <textarea id="Adress" class="no-bor form-control" disabled></textarea>
                    </div>
                    <br>
                    <div class="input-group">
                      <span class="input-group-text textbox4 mg-0 f-9">End Customer:</span>
                      <input type="text" class="form-control edit f-9" id="PJ_End_Customer" disabled>
                    </div>
                    <hr>
                    <span class="input-group-text group-title mg-0 f-9">Condition</span>

                    <div class="input-group">
                      <span class="input-group-text textbox4 mg-0 f-9">Validity:</span>
                      <input type="text" class="form-control edit  f-9" id="PJ_Validity" disabled>
                      <span class="input-group-text textbox2 mg-0 f-9">Delivery:</span>
                      <input type="text" class="form-control edit f-9" id="PJ_Delivery" disabled>
                    </div>

                    <div class="input-group">
                      <span class="input-group-text textbox4 mg-0 f-9">Term of Payment:</span>
                      <input type="text" class="form-control edit f-9" id="PJ_Payment1" disabled>
                    </div>

                    <div class="input-group">
                      <span class="input-group-text textbox4 mg-0 f-9"></span>
                      <input type="text" class="form-control edit f-9" id="PJ_Payment2" disabled>
                    </div>

                    <div class="input-group">
                      <span class="input-group-text textbox4 mg-0 f-9"></span>
                      <input type="text" class="form-control edit f-9" id="PJ_Payment3" disabled>
                    </div>

                    <div class="input-group">
                      <span class="input-group-text textbox4 mg-0 f-9">Remark:</span>
                      <input type="text" class="form-control edit f-9" id="PJ_Remark" disabled>
                    </div>

                    <hr>
                    <span class="input-group-text group-title mg-0 f-9">Price</span>

                    <div class="input-group ">
                      <span class="input-group-text mg-0">Sub Total:</span>
                      <input id="TotalPrice" class="no-bor form-control" disabled>
                    </div>

                    <div class="input-group ">
                      <span class="input-group-text textbox2 mg-0 f-9">Discount: </span>
                      <input type="text" class="form-control edit f-9" id="PJ_Discount" disabled>
                    </div>

                    <div class="input-group ">
                      <span class="input-group-text mg-0">Price after discount:</span>
                      <input id="PriceAfter" class="no-bor form-control" disabled>
                    </div>

                    <div class="input-group ">
                      <span class="input-group-text mg-0">VAT Including 7%:</span>
                      <input id="Vat" class="no-bor form-control" disabled>
                    </div>

                    <div class="input-group ">
                      <span class="input-group-text mg-0">Net Total:</span>
                      <input id="NetTotal" class="no-bor form-control" disabled>
